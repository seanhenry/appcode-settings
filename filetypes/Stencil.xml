<filetype binary="false" description="Stencil templating" name="Stencil">
  <highlighting>
    <options>
      <option name="LINE_COMMENT" value="" />
      <option name="COMMENT_START" value="{#" />
      <option name="COMMENT_END" value="#}" />
      <option name="HEX_PREFIX" value="" />
      <option name="NUM_POSTFIXES" value="" />
      <option name="HAS_PARENS" value="true" />
    </options>
    <keywords keywords="block;endblock;endfor;endif;extends;for;if;in" ignore_case="false" />
  </highlighting>
  <extensionMap>
    <mapping ext="stencil" />
  </extensionMap>
</filetype>